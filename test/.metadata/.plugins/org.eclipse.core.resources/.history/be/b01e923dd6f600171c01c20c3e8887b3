package o2o.enums;

public enum ShopStateEnum {
  CHECK(0,"审核中");
  
  private int state;
  private String info;
  private ShopStateEnum(Integer state,String info){
    this.state = state;
    this.info = info;
  }
  
  public static String stateOf(int state){
    for (ShopStateEnum stateEnum : values()) {
      if( stateEnum.get==  state )
    }
  }

  public int getState() {
    return state;
  }

  public String getInfo() {
    return info;
  }

  
  
}
